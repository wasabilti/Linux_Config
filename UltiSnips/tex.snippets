snippet "bf" "begin{figure} / end{}" br
\begin{figure}[ht]
\centering
\includegraphics[height=${1:4}cm]{${2:filename}}
\caption{${4:captionname}}
\label{${3:label_name}}
\end{figure}$0
endsnippet

snippet "bsf2" "begin two figure with 2 subfigure" br
\begin{figure}[ht]
\centering
	\subfigure[${1:}]{
		\includegraphics[height=${2:4}cm]{${3:filename_}}
	}
	\subfigure[${4:}]{
		\includegraphics[height=$2cm]{$3}
	}
\caption{${5:captionname}}
\label{${4:filename}}
\end{figure}$0
endsnippet


